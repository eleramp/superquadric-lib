<application>
    <name>Superquadric Lib Demo</name>

    <dependencies>
        <port>/pointCloudRead/rpc</port>
        <port>/actionsRenderingEngine/cmd:io</port>
        <port>/actionsRenderingEngine/get:io</port>
        <port>/iolReachingCalibration/rpc</port>
        <port>/SFM/rpc</port>
        <port>/memory/rpc</port>
        <port>/icub/camcalib/left/out</port>
        <port>/lbpExtract/rpc:i</port>
        <port>/icub/camcalib/left/out</port>
        <port>/icub/camcalib/right/out</port>
        <port>/icub/cartesianController/left_arm/state:o</port>
        <port>/icub/cartesianController/right_arm/state:o</port>
        <port>/wholeBodyDynamics/left_arm/cartesianEndEffectorWrench:o</port>
        <port>/wholeBodyDynamics/right_arm/cartesianEndEffectorWrench:o</port>
        <port>/iKinGazeCtrl/rpc</port>
    </dependencies>
    <module>
        <name>SFM</name>
        <node>icub22</node>
    </module>
    <module>
        <name>Superquadric-Lib-Demo</name>
        <node>localhost</node>
    </module>
    <module>
        <name>actionsRenderingEngine</name>
        <parameters>--motor::block_eyes 5.0 --from config_superq.ini</parameters>
        <node>icub22</node>
    </module>
    <module>
        <name>iolReachingCalibration</name>
        <node>icub22</node>
    </module>
    <module>
        <name>iolStateMachineHandler</name>
        <node>icub22</node>
    </module>
    <module>
        <name>point-cloud-read</name>
        <node>localhost</node>
    </module>
    <module>
        <name>yarpview</name>
        <parameters>--name /iolViewer/disparity --x 0 --y 0 --p 50 --compact</parameters>
        <node>icub22</node>
    </module>
        <module>
                <name>yarpview</name>
                <parameters>--name /iolViewer/manager/img --x 370 --y 720 --p 50 --compact</parameters>
                <node>icub22</node>
        </module>
    <connection>
        <from>/superquadric-lib-demo/pointCloud:rpc</from>
        <to>/pointCloudRead/rpc</to>
        <protocol>fast_tcp</protocol>
    </connection>

    <connection>
        <from>/superquadric-lib-demo/actionRenderer:rpc</from>
        <to>/actionsRenderingEngine/cmd:io</to>
        <protocol>fast_tcp</protocol>
    </connection>

    <connection>
        <from>/superquadric-lib-demo/tableCalib:rpc</from>
        <to>/actionsRenderingEngine/get:io</to>
        <protocol>fast_tcp</protocol>
    </connection>

    <connection>
        <from>/superquadric-lib-demo/sfm:rpc</from>
        <to>/SFM/rpc</to>
        <protocol>fast_tcp</protocol>
    </connection>
        <connection>
                <from>/icub/camcalib/left/out</from>
                <to>/iolStateMachineHandler/img:i</to>
                <protocol>fast_tcp</protocol>
        </connection>
    <connection>
        <from>/superquadric-lib-demo/reachingCalibration:rpc</from>
        <to>/iolReachingCalibration/rpc</to>
        <protocol>fast_tcp</protocol>
    </connection>

    <connection>
      <from>/icub/camcalib/left/out</from>
      <to>/pointCloudRead/imgL:i</to>
      <protocol>udp</protocol>
  </connection>

  <connection>
      <from>/pointCloudRead/OPCrpc</from>
      <to>/memory/rpc</to>
      <protocol>fast_tcp</protocol>
  </connection>

  <connection>
      <from>/pointCloudRead/SFMrpc</from>
      <to>/SFM/rpc</to>
      <protocol>fast_tcp</protocol>
  </connection>

  <connection>
      <from>/pointCloudRead/segmrpc</from>
      <to>/lbpExtract/rpc:i</to>
      <protocol>fast_tcp</protocol>
  </connection>

  <connection>
      <from>/iolStateMachineHandler/motor_grasp:rpc</from>
      <to>/superquadric-lib-demo/cmd:rpc</to>
      <protocol>fast_tcp</protocol>
  </connection>
        <connection>
                <from>/SFM/disp:o</from>
                <to>/iolViewer/disparity</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/icub/camcalib/left/out</from>
                <to>/iolViewer/manager/img</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/iolReachingCalibration/opc</from>
                <to>/memory/rpc</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/iolReachingCalibration/are</from>
                <to>/actionsRenderingEngine/cmd:io</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/iolStateMachineHandler/reach_calib:rpc</from>
                <to>/iolReachingCalibration/rpc</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/iolStateMachineHandler/motor:rpc</from>
                <to>/actionsRenderingEngine/cmd:io</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/iolStateMachineHandler/motor_stop:rpc</from>
                <to>/actionsRenderingEngine/rpc</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/actionsRenderingEngine/wbd:rpc</from>
                <to>/wholeBodyDynamics/rpc:i</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/iolStateMachineHandler/memory:rpc</from>
                <to>/memory/rpc</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/actionsRenderingEngine/OPC:io</from>
                <to>/memory/rpc</to>
                <protocol>fast_tcp</protocol>
        </connection>
        <connection>
                <from>/icub/camcalib/right/out</from>
                <to>/SFM/right:i</to>
                <protocol>mjpeg</protocol>
        </connection>
        <connection>
                <from>/icub/camcalib/left/out</from>
                <to>/SFM/left:i</to>
                <protocol>mjpeg</protocol>
        </connection>
        <connection>
                <from>/iolStateMachineHandler/get3d:rpc</from>
                <to>/SFM/rpc</to>
                <protocol>fast_tcp</protocol>
        </connection>

</application>
